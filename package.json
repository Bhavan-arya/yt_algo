{
  "name": "youtube-algorithm-analyzer",
  "version": "1.0.0",
  "description": "YouTube algorithm that analyzes user keystrokes to provide creator feedback",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "dev-all": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["youtube", "algorithm", "analytics", "keystroke", "creator-feedback"],
  "author": "Your Name",
  "license": "MIT"
}
